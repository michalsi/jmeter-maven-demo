                      <stringProp name="cacheKey">true</stringProp>
                      <stringProp name="filename"></stringProp>
                      <stringProp name="parameters"></stringProp>
                      <stringProp name="script">function getRandomInt(min, max) {
    return Math.floor(Math.random() * (max - min + 1)) + min;
}

//Get random incom budget node from the available range
var incomeRange = vars.get(&quot;incomeBudget_matchNr&quot;);
var randomIndex = getRandomInt(1,incomeRange);
var incomeBudget = vars.get(&quot;incomeBudget_&quot;+randomIndex);

var multiplier = 10000;

var projected = Math.random() * multiplier ;
var actual = Math.random() * multiplier;
var remaining = projected - actual;

//variable is saved in String - we need to convert it to JSON object
incomeObj = JSON.parse(incomeBudget);

//modify existing keys
incomeObj.projected = parseFloat(projected).toFixed(2);
incomeObj.actual = parseFloat(actual).toFixed(2);
incomeObj.remaining = parseFloat(remaining).toFixed(2);

//now we need to convert object back to String to save it in variable
vars.put(&quot;updatedIncomeBudget&quot;, JSON.stringify(incomeObj) );

//additionally extract budget ID into separate variable
vars.put(&quot;incomeBudgetId&quot;,JSON.stringify(incomeObj.id) );</stringProp>
                      <stringProp name="scriptLanguage">javascript</stringProp>
                    </JSR223PreProcessor>
